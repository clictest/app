<?xml version='1.0' encoding='utf-8'?>
<widget id="com.myapp.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets"
	xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>Clic Test</name>

	<platform name="android">
		<icon src="www/images/qliktest-logo.png" />
	</platform>
	
	<description>
		A sample Apache Cordova application that responds to the
		deviceready
		event.
	</description>

	<author href="http://www.eclipse.org/thym" email="thym-dev@eclipse.org">
		Eclipse.org -
		Thym
	</author>

	<content src="shadow.html" />
	<!-- Whitelist configuration. Refer to https://cordova.apache.org/docs/en/edge/guide_appdev_whitelist_index.md.html -->
	<plugin name="cordova-plugin-whitelist" spec="1" />

    <access origin="https://www.facebook.com/" />
	<access origin="https://www.google.com/" />
	<access origin="https://www.map.google.com/" />
	<access origin="https://www.yahoo.com/" />
	<access origin="*" />

	<allow-intent href="https://www.facebook.com/" />
	<allow-intent href="https://www.google.com/" />
	<allow-intent href="https://www.map.google.com/" />
	<allow-intent href="https://www.yahoo.com/" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
	<engine name="android" version="5.1.0" />
	<feature name="InAppBrowser">
		<param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
		<param name="wp-package" value="InAppBrowser" />
		<param name="id" value="cordova-plugin-inappbrowser" />
	</feature>
	<feature name="Console">
		<param name="wp-package" value="DebugConsole" />
		<param name="id" value="cordova-plugin-console" />
	</feature>
	

</widget>
